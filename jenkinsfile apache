// bring the code from scm

 stages {
    stage("checkout"){
          get clone url

    }

// zip the file
stage("CI")
{
 zip -r name.zip index.html

// send the zip file to the artifactory
 
 aws s3 cp name.zip s3://bucket name
}
//cd
//bring the code from artifactory
stage("CD")
 {aws s3 cp s3://bucket name/file name .

//unzip the file

 unzip file.zip

// send to ec2

scp file root@privateip ':/var/www/html/'
}

 }